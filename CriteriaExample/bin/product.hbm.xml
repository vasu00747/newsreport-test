<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.asminds.criteria.bean.Productss" table="products1">
<id name="pid" column="prid"  />
<property name="pname" column="prname" length="10"/>
<property name="price"/>

</class>

<query name="myquery">
<![CDATA[from Productss p where p.price>:java4s]]>
</query>

</hibernate-mapping>
